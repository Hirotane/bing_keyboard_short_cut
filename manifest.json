{
    "name": "__MSG_Name__",
    "version": "1.0.0",
    "manifest_version": 2,
    "default_locale": "en",
    "description": "__MSG_Description__",
    "icons": {
      "16": "icons/Bing_shortcut_16x16.png",
      "32": "icons/Bing_shortcut_32x32.png",
      "48": "icons/Bing_shortcut_48x48.png",
      "128": "icons/Bing_shortcut_128x128.png"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [{
      "matches": ["*://www.bing.com/search*"],
      "js": [
        "utils.js",
        "shortcuts.js"
      ]
    },{
      "exclude_matches": ["*://www.bing.com/search*"],
      "matches": ["<all_urls>"],
      "js": [
        "utils.js",
        "outside_search_pages.js"
      ]
    }]
  }